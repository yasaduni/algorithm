package sort;
	import java.util.LinkedList;
	import java.util.Queue;

public class FIFOQueueLinkedList {
	    public static void main(String[] args) {
	        Queue<Integer> queue = new LinkedList<>();

	        // Enqueue elements
	        queue.add(1);
	        queue.add(2);
	        queue.add(3);

	        // Dequeue elements
	        System.out.println("Dequeued: " + queue.poll()); // Output: Dequeued: 1
	        System.out.println("Dequeued: " + queue.poll()); // Output: Dequeued: 2

	        // Peek at the front of the queue
	        System.out.println("Front of the queue: " + queue.peek()); // Output: Front of the queue: 3
	    }
	}
